{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_name": {
      "type": "string"
    },
    "product_description": {
      "type": "string"
    },
    "categories": {
      "type": "enumeration",
      "enum": [
        "MEAT",
        "VEGETABLES",
        "DRINKS"
      ]
    },
    "stock": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "Price": {
      "type": "decimal",
      "default": 0.1
    }
  }
}
